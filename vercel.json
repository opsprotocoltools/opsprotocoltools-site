{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/next"
        }
    ],
    "env": {
        "NEXT_PUBLIC_APP_NAME": "Ops Protocol Tools"
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}